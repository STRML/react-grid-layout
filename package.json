{
  "name": "heidi-grid-layout",
  "version": "0.13.901",
  "description": "A draggable and resizable grid layout with responsive breakpoints, for React.",
  "main": "index.js",
  "scripts": {
    "clean-build": "if exist build rd /q /s build 2>nul",
    "clean-dist": "if exist dist rd /q /s dist 2>nul",
