    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-hot-loader": "^1.3.0",
    "react-transform-hmr": "^1.0.2",
    "style-loader": "^0.13.0",
    "valiquire": "^0.3.0",
